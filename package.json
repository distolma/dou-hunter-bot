{
  "name": "dou-hunter-bot",
  "version": "1.0.0",
  "description": "Telegram bot, which helps you to find IT vacancies on DOU.ua",
  "repository": "git@github.com:distolma/dou-hunter-bot.git",
  "author": "distolma <djmakaron2009@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.2.4",
    "@nestjs/core": "^6.10.14",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/platform-express": "^6.10.14",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cookie": "^0.4.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.2",
    "nestjs-telegraf": "^0.7.2",
    "node-emoji": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "system-sleep": "^1.3.6",
    "telegraf": "^3.36.0",
    "telegraf-session-local": "^1.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/cheerio": "^0.22.16",
    "@types/cookie": "^0.3.3",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.2",
    "@types/node": "^13.1.6",
    "@types/node-emoji": "^1.8.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
